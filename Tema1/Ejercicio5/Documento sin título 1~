/**
 * Muestra por pantalla la frase Nombre,calle,numero
 *
 * @author isaias
 */
public class Colores {
public static void main(String[] args) {
String rojo = "\033[31m";
String verde = "\033[32m";
String naranja = "\033[33m";
String azul = "\033[34m";
String morado = "\033[35m";
String blanco = "\033[37m";

public class Colores { // Clase principal
  public static void main(String[] args) {
    System.out.println(Naranja+"\t\tLunes\tMartes\tMiercoles\tJueves\tViernes");
    System.out.println("8:15-9:15\tFol\tProgr\tSin\t\tProg\tBD");
    System.out.println("9:15-10:15\tFol\tProg\tSin\t\tProg\tBD");
    System.out.println("10:15-11:15\tFol\tProg\tSin\t\tProgr\tBD");
    System.out.println("11:45-12:45\tProg\tProg\tSin\t\tED\tProg");
    System.out.println("12:45-13:45\tED\tBD\tLm\t\tLm\tSin");
    System.out.println("13:45-14:45\tED\tBD\tLm\t\tLm\tSin");
     }
}





